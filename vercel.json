{
  "builds": [
    {
      "src": "mobile/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build:web",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
